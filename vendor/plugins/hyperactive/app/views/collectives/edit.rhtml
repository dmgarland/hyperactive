<h1>Editing collective</h1>

<%= error_messages_for :collective %>

<% form_for(:collective, :url => collective_path(@collective), :html => { :method => :put, :multipart => true }) do |f| %>

  <%= render :partial => "collectives/form", :locals => {:f => f} %>

  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', collective_path(@collective) %> |
<%= link_to 'Back', collectives_path %>