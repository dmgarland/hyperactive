<h1><%= "New Video".t %></h1>
<% form_for(:content, :url => videos_path, :html => {:multipart => true}) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= render(:partial => 'shared/captcha') %>
  <%= submit_tag "Create".t %>
  <%= "Please only click once, and be patient, video uploading can take a while.".t %>
<% end %>	