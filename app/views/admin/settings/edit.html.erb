<h2>Edit setting</h2>

<% form_for(@setting, :url => admin_setting_url(@setting), :html => { :method => :put }) do |f| %>
<%= f.error_messages %>

<%= render :partial => 'form', :locals => {:f => f} %>
  
<p>
<%= f.submit "Update setting" %>
</p>
<% end %>

<p>
<%= link_to 'Show setting', admin_setting_url(@setting) %> |
<%= link_to 'Back to settings', admin_settings_url %>
</p>