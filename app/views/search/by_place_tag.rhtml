<% if !@content.nil? %>
    <% if !@content.empty? %>
    <h1>In &lsquo;<%= h @tagname %>&rsquo;</h1>
        <%= link_to (image_tag('feed-icon-28x28.png')), upcoming_events_by_place_feed_path(params[:scope]) %>
        <%= render(:partial => "shared/content/list_summary", :collection => @content) %>
        <%= page_links(@content) %>
    <% else %>
        <%= I18n.t 'search.past_events_explanation' %>
    <% end %>
<% else %>
	<%= I18n.t 'search.no_tags_found', :tag => @tagname %>
<% end %>
