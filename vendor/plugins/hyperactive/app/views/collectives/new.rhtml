<h1>New collective</h1>

<%= error_messages_for :collective %>

<% form_for(:collective, :url => collectives_path, :html => {:multipart => true}) do |f| %>
  
  <%= render :partial => "collectives/form", :locals => {:f => f}  %>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', collectives_path %>