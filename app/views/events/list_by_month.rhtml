<h1><%= "#{@date.strftime('%B')}"%> <%= @date.year %></h1>
<p><%= link_to('<<', :date=>@date.last_year) %>
   <%= link_to('<', :date=>@date.last_month) %>
   <%= link_to('>', :date=>@date.next_month) %>
   <%= link_to('>>', :date=>@date.next_year) %>  
</p> 
<%= render(:partial => "shared/content/list_summary", :collection => @content) %>
<% content_for :sidebar do %>
	<%= render :partial => "events/sidebar/feedbox" %>
<% end %>
