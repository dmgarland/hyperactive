<h1><%= long_date(@date) %> to <%= long_date(@date + 6) %></h1>
<p><%= link_to("Previous week: #{(@date-7).strftime('%D')}", :date=>@date-7) %>
   <%= link_to("Next week: #{(@date+7).strftime('%D')}", :date=>@date+7) %>
</p> 
<%= render(:partial => "shared/content/list_summary", :collection => @content) %>
<%= paginating_links(@content, :params => params) %>