<h1>New snippet</h1>

<%= error_messages_for :snippet %>

<% form_for(:snippet, :url => admin_snippets_path, :html => {:multipart => true}) do |f| %>

  <%= render :partial => 'form', :locals => {:f => f} %>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_snippets_path %>