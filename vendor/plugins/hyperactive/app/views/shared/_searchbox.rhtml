<div id="user_info_div"></div>
<script type="text/javascript">
  UserInfo.transferFromCookies();
  UserInfo.writeDataTo('error', $('user_info_div'));
</script>
<div>
<% form_tag(search_url, :method => :get) do %>
    <%= text_field(:search, :search_terms, :size => 15) %>
    <%= submit_tag 'Search'.t %>
<% end %>
</div>