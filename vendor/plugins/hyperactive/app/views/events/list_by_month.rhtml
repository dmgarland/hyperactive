<h1><%= "#{@date.strftime('%B')}"%> <%= @date.year %></h1>
<p><%= link_to('<<', :date=>@date.<<(12)) %>
   <%= link_to('<', :date=>@date.<<(1)) %>
   <%= link_to('>', :date=>@date.>>(1)) %>
   <%= link_to('>>', :date=>@date.>>(12)) %>  
</p> 
<%= render(:partial => "shared/content/list_summary", :collection => @content) %>
