<h1><%= I18n.t('videos.new.title') %></h1>
<% form_for(:content, :url => videos_path, :html => {:multipart => true}) do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= render(:partial => 'shared/captcha') %>
  <%= submit_tag I18n.t('videos.new.create') %>
  <%= I18n.t('videos.new.click_once') %>
<% end %>

